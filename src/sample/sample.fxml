<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mainVbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0">
         <children>
            <Label text="PSM Data (.csv) To Video Data (.mp4)">
               <font>
                  <Font name="Book Antiqua Bold" size="27.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="200.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="600.0">
               <children>
                  <VBox alignment="BOTTOM_LEFT" maxHeight="-Infinity" prefHeight="30.0" prefWidth="600.0">
                     <children>
                        <Label fx:id="loadingLabel" text="Loading Bar" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </VBox>
                  <ProgressBar fx:id="loadingBar" prefWidth="580.0" progress="0.0">
                     <VBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </VBox.margin>
                  </ProgressBar>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" maxHeight="-Infinity" prefHeight="100.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label fx:id="selectPSMFIleLabel" text="Select PSM File" />
                  <Button fx:id="loadFileButton" mnemonicParsing="false" text="Load" />
               </children>
            </VBox>
         </children></HBox>
   </children>
</VBox>
